  "dependencies": {
    "@mdx-js/mdx": "^2.2.1",
    "@octokit/rest": "^19.0.5",
    "@pnpm/find-workspace-dir": "^5.0.1",
    "@types/semver": "^7.3.13",
    "chalk-template": "^0.5.0",
    "json-schema": "^0.4.0",
    "remark": "^14.0.2",
    "remark-gfm": "^3.0.1",
    "remark-mdx": "^2.2.1",
    "semver": "^7.3.8",
    "unified": "^10.1.2",
    "unist-builder": "^3.0.0",
    "unist-util-visit": "^4.1.1"
  }
